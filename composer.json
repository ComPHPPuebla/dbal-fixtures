{
    "name": "comphppuebla/dbal-fixtures",
    "description": "Fixtures managament with Doctrine DBAL",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/dbal": "^2.5",
        "symfony/console": "^3.1",
        "symfony/yaml": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "ComPHPPuebla\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod u+x bin/cli"
        ],
        "post-update-cmd": [
            "chmod u+x bin/cli"
        ]
    }
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
